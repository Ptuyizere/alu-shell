#!/usr/bin/env bash
# Stop the process running the script '4-to_infinity_and_beyond' using kill
pid=$(pgrep -f "4-to_infinity_and_beyond")
if [[ -n "$pid" ]]; then
    kill "$pid"
    echo "Process 4-to_infinity_and_beyond (PID: $pid) has been stopped."
else
    echo "No process found running 4-to_infinity_and_beyond."
fi
